.uni-popup-dialog {
    background-color: #fff;
    border-radius: 11px;
    width: 300px
}

.uni-dialog-title {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-justify-content: center;
    justify-content: center;
    padding-top: 25px
}

.uni-dialog-title-text {
    font-size: 16px;
    font-weight: 500
}

.uni-dialog-content {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-justify-content: center;
    justify-content: center;
    padding: 20px
}

.uni-dialog-content-text {
    color: #6c6c6c;
    font-size: 14px
}

.uni-dialog-button-group {
    border-top: 1px solid #f5f5f5;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row
}

.uni-dialog-button {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-flex: 1;
    flex: 1;
    -webkit-flex-direction: row;
    flex-direction: row;
    height: 45px;
    -webkit-justify-content: center;
    justify-content: center
}

.uni-border-left {
    border-left: 1px solid #f0f0f0
}

.uni-dialog-button-text {
    color: #333;
    font-size: 16px
}

.uni-button-color {
    color: #007aff
}

.uni-dialog-input {
    border: 1px solid #eee;
    border-radius: 5px;
    color: #555;
    -webkit-flex: 1;
    flex: 1;
    font-size: 14px;
    height: 40px;
    padding: 0 10px
}

.uni-popup__success {
    color: #4cd964
}

.uni-popup__warn {
    color: #f0ad4e
}

.uni-popup__error {
    color: #dd524d
}

.uni-popup__info {
    color: #909399
}
